        FX 'REML.FX.bt+tlrc.HEAD[0]' 'REML.FX.bt+tlrc.HEAD[1]'  \
        GF 'REML.GF.bt+tlrc.HEAD[0]' 'REML.GF.bt+tlrc.HEAD[1]'  \
        GG 'REML.GG.bt+tlrc.HEAD[0]' 'REML.GG.bt+tlrc.HEAD[1]'  \
        GI 'REML.GI.bt+tlrc.HEAD[0]' 'REML.GI.bt+tlrc.HEAD[1]'  \
        GK 'REML.GK.bt+tlrc.HEAD[0]' 'REML.GK.bt+tlrc.HEAD[1]'  \
